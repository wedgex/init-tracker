{
    "extends": "airbnb",
    "plugins": [
        "react",
        "import"
    ],
    "ecmaFeatures": {
      "classes": true
    },
    "env": {
      "jest": true,
      "browser": true
    },
    "rules": {
      "semi": 0,
      "arrow-parens": [2, "as-needed"],
      "react/require-default-props": 0,
      "react/jsx-filename-extension": 0
    }
}
